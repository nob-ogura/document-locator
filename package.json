{
  "name": "document-locator",
  "version": "0.1.0",
  "description": "Document locator CLI project scaffolded for Node.js 22 with pnpm.",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "crawler": "node --experimental-strip-types src/cli/crawler.ts",
    "search": "node --experimental-strip-types src/cli/search.ts",
    "db:apply": "node scripts/db-apply.mjs",
    "format": "biome format --write .",
    "lint": "biome check --write --assist-enabled=true .",
    "typecheck": "tsc --noEmit",
    "test": "node scripts/test-runner.mjs",
    "verify": "pnpm format && pnpm lint && pnpm typecheck && pnpm test"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.83.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.6",
    "@types/node": "^22.9.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.10"
  }
}
