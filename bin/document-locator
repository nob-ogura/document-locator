#!/usr/bin/env node

// Entry point script for the document-locator CLI.
// This expects the TypeScript sources to be compiled to dist/ via `npm run build`.
// eslint-disable-next-line @typescript-eslint/no-var-requires
const cli = require('../dist/cli.js');

if (typeof cli.runCli === 'function') {
  cli.runCli(process.argv);
} else {
  console.error('document-locator CLI is not built. Run `npm run build` first.');
  process.exit(1);
}

